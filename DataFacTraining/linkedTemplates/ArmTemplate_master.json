{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFacTraining"
		},
		"MySqlDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySqlDb'"
		},
		"MyStorageAccount_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'MyStorageAccount'"
		},
		"MyKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://MyKVTraining.vault.azure.net/"
		},
		"MyRestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://universities.hipolabs.com/"
		},
		"MyStorageAccount_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mytrainings.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFacTraining_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MySqlDb_connectionString": {
						"value": "[parameters('MySqlDb_connectionString')]"
					},
					"MyStorageAccount_accountKey": {
						"value": "[parameters('MyStorageAccount_accountKey')]"
					},
					"MyKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('MyKeyVault_properties_typeProperties_baseUrl')]"
					},
					"MyRestAPI_properties_typeProperties_url": {
						"value": "[parameters('MyRestAPI_properties_typeProperties_url')]"
					},
					"MyStorageAccount_properties_typeProperties_url": {
						"value": "[parameters('MyStorageAccount_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DataFacTraining_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DataFacTraining_ArmTemplate_0')]"
			]
		}
	]
}